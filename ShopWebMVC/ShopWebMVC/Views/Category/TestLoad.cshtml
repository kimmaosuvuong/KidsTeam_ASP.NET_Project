@model List<ShopWebMVC.Models.CategoryModel>
@{
    ViewBag.Title = "TestLoad";
    Layout = "~/Views/Shared/MyLayout.cshtml";
}

<h2>TestLoad</h2>

<table>
    <tr>
        <th>
            Id
        </th>
        <th>
           Name
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
    }
</table> 

